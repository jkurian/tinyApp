<!DOCTYPE html>
<html lang="en">
<head>
    <% include ./partials/_header %>
</head>
<body>
    <table>
            <tr>
                <td>URLs</td>
                <td>Shortened URLs</td>
            </tr>
            <% for(let url in urlList) { %>
                <tr>
                    <td style="padding-right: 1em"> <%= urlList[url] %> </td>
                    <td style="padding-right: 1em"> <%= url %> </td>
                    <td style="padding-left: 1em"><form id="deleteURL" method="POST" action="/urls/<%= url %>/delete"><button type="submit" form="deleteURL">Delete URL <i class="icon-trash icon-small"></i></button></form></td>
                </tr>
            <% }; %>
        </table>
</body>
</html>